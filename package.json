{
  "name": "using-preact",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "update": "npx npm-check-updates -u",
    "load_schema": "graphqurl $GQL_HOST -H 'x-hasura-admin-secret: '$GQL_TOKEN --introspect > ./src/schema.graphql",
    "update_schema": "dotenv -e .env.local -- npm run load_schema",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-generic-sdk": "^1.17.9",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/node": "^14.14.31",
    "dotenv-cli": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "graphqurl": "^0.3.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prefresh/next": "^1.3.0",
    "@reatom/core": "^2.0.0-alpha10",
    "@reatom/react": "^2.0.0-alpha2",
    "@types/bcrypt": "^3.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/use-subscription": "^1.0.0",
    "bcrypt": "^5.0.1",
    "graphql-request": "^3.4.0",
    "nes.css": "^2.3.0",
    "next": "^10.0.0",
    "next-plugin-preact": "^3.0.3",
    "preact": "^10.5.5",
    "preact-render-to-string": "^5.1.11",
    "react": "npm:@preact/compat@^0.0.3",
    "react-dom": "npm:@preact/compat@^0.0.3",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.2",
    "stylerun": "^0.0.1-rc1",
    "use-subscription": "^1.5.1"
  },
  "license": "MIT"
}
